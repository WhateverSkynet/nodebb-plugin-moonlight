{
  "name": "nodebb-plugin-moonlight",
  "version": "0.2.13",
  "description": "Battle.net integration with Nodebb",
  "main": "library.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/WhateverSkynet/nodebb-plugin-moonlight"
  },
  "scripts": {
    "build": "set NODE_ENV=production&&webpack -p",
    "test": "jest"
  },
  "keywords": [
    "nodebb",
    "plugin",
    "battlenet",
    "wow"
  ],
  "license": "MIT",
  "readmeFilename": "README.md",
  "nbbpm": {
    "compatibility": "^1.0.0",
    "index": true
  },
  "devDependencies": {
    "@types/jest": "18.1.1",
    "@types/lodash": "4.14.52",
    "@types/react": "0.14.41",
    "@types/react-addons-pure-render-mixin": "0.14.14",
    "@types/react-dom": "0.14.18",
    "@types/react-redux": "4.4.32",
    "@types/react-router": "3.0.2",
    "@types/react-tap-event-plugin": "0.0.30",
    "@types/redux-form": "6.3.2",
    "@types/uuid": "2.0.29",
    "core-js": "2.4.1",
    "css-loader": "0.26.1",
    "extract-text-webpack-plugin": "2.0.0-rc.3",
    "image-webpack-loader": "3.2.0",
    "jest": "20.0.0",
    "jest-cli": "20.0.0",
    "lodash": "4.17.4",
    "material-ui": "0.17.4",
    "node-sass": "4.5.0",
    "normalize.css": "5.0.0",
    "normalizr": "2.2.1",
    "react": "15.4.2",
    "react-addons-css-transition-group": "15.4.2",
    "react-addons-perf": "15.4.2",
    "react-addons-pure-render-mixin": "15.4.2",
    "react-css-themr": "1.1.3",
    "react-dom": "15.4.2",
    "react-redux": "5.0.2",
    "react-test-renderer": "15.4.2",
    "react-router": "3.0.2",
    "react-router-redux": "4.0.7",
    "react-tap-event-plugin": "2.0.1",
    "redux": "3.6.0",
    "redux-devtools-extension": "2.13.0",
    "redux-form": "6.5.0",
    "redux-observable": "0.13.0",
    "rxjs": "5.1.0",
    "sass-loader": "5.0.1",
    "source-map-loader": "0.1.6",
    "style-loader": "0.13.1",
    "tape": "4.6.3",
    "ts-jest": "20.0.2",
    "ts-loader": "2.0.0",
    "typescript": "2.1.6",
    "webpack": "2.2.0"
  },
  "dependencies": {
    "uuid": "3.0.1",
    "battlenet-api": "0.13.0"
  },
  "jest": {
    "transform": {
      ".(ts|tsx)": "<rootDir>/node_modules/ts-jest/preprocessor.js"
    },
    "testRegex": "(/__tests__/.*|\\.(test|spec))\\.(ts|tsx|js)$",
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/fileMock.js"
    },
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "globals": {
      "__TS_CONFIG__": {
        "module": "commonjs",
        "jsx": "react"
      }
    }
  }
}